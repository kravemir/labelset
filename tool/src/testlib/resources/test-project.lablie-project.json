{
  "datasets": [
    {
      "key": "products",
      "jsonCollectionStorage": {
        "location": "src/products"
      }
    },
    {
      "key": "series",
      "csvTableStorage": {
        "location": "src/series.csv"
      }
    }
  ],
  "outputsets": [
    {
      "key": "products",
      "template": "src/product.svg",
      "dataset": "products",
      "outputMimetypes": ["pdf"],
      "paper": {
        "with": 210,
        "height": 297,
        "tileOffsetX": 5,
        "tileOffsetY": 8.5,
        "tileWidth": 50,
        "tileHeight": 40
      }
    },
    {
      "key": "products_with_svg",
      "template": "src/product.svg",
      "dataset": "products",
      "outputMimetypes": ["pdf", "svg"],
      "paper": {
        "with": 210,
        "height": 297,
        "tileOffsetX": 5,
        "tileOffsetY": 8.5,
        "tileWidth": 50,
        "tileHeight": 40
      }
    },
    {
      "key": "series",
      "template": "src/series.svg",
      "dataset": "series",
      "outputMimetypes": ["pdf","svg"],
      "paper": {
        "with": 210,
        "height": 297,
        "tileOffsetX": 5,
        "tileOffsetY": 8.5,
        "tileWidth": 50,
        "tileHeight": 40
      }
    }
  ]
}
