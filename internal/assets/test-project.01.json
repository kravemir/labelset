{
  "datasets": {
    "products": {
      "jsonCollectionStorage": {
        "location": "src/products"
      }
    },
    "series": {
      "jsonCollectionStorage": {
        "location": "src/series"
      }
    }
  },
  "outputsets": {
    "products": {
      "template": "src/product.svg",
      "dataset": "products",
      "outputMimetypes": [
        "pdf"
      ],
      "paper": {
        "width": 210,
        "height": 297,
        "tileOffset": {
          "X": 5,
          "Y": 8.5
        },
        "tileSize": {
          "Width":  50,
          "Height": 40
        }
      }
    },
    "products_with_svg": {
      "template": "src/product.svg",
      "dataset": "products",
      "outputMimetypes": [
        "pdf",
        "svg"
      ],
      "paper": {
        "width": 210,
        "height": 297,
        "tileOffset": {
          "X": 1,
          "Y": 2
        },
        "tileSize": {
          "Width":  22,
          "Height": 23
        }
      }
    },
    "series": {
      "template": "src/series.svg",
      "dataset": "series",
      "outputMimetypes": [
        "pdf",
        "svg"
      ],
      "paper": {
        "width": 210,
        "height": 297,
        "tileOffset": {
          "X": 4,
          "Y": 5
        },
        "tileSize": {
          "Width":  42,
          "Height": 54
        }
      }
    }
